<!DOCTYPE html>
<html>
<head>
  <title>Testproject</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user.present? %>
<%= link_to 'マイページに戻る', book_records_path %>
<%= link_to 'ユーザ情報編集',edit_user_path(session[:user_id]) %>
<%= link_to 'ログアウト', {controller: :logins,action: :destroy}, method: :delete%>
<% else %>
<%= link_to 'ログイン', login_path %>
<% end %>
<%= yield %>

</body>
</html>
